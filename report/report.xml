            <field name="name">Purchase Request Excel</field>
            <field name="model">purchase.request</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">purchase_request.report_purchase_request_xlsx</field>
            <field name="report_file">purchase_request.report_purchase_request_xlsx</field>
            <field name="binding_model_id" ref="model_purchase_request"/>
            <field name="binding_type">report</field>
